{
  "name": "create-fast-api",
  "version": "1.0.0",
  "description": "CLI tool to create Node.js REST APIs",
  "main": "dist/index.js",
  "bin": {
    "create-fast-api": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && cpy \"src/templates/**/*\" \"!src/templates/**/node_modules/**\" \"!src/templates/**/dist/**\" \"dist/templates/\"",
    "prebuild": "rm -rf ./dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cli",
    "nodejs",
    "api",
    "rest",
    "typescript",
    "javascript",
    "generator",
    "scaffold",
    "boilerplate"
  ],
  "author": "Toraxa Studio",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "chalk": "^5.4.1",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.12",
    "cpy-cli": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*",
    "src/templates/**/*"
  ],
  "engines": {
    "node": ">=16"
  }
}
